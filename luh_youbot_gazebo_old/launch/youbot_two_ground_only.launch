<?xml version="1.0" ?>
<launch>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find luh_youbot_gazebo)/worlds/youbot_single_exit.world"/>
    <!-- <arg name="world_name" value="$(find luh_youbot_gazebo_0)/worlds/youbot_empty.world"/> -->
  </include>

  <!-- Convert an xacro and put on parameter server -->

  <!-- <param name="robot_description_0" command="$(find xacro)/xacro.py $(find luh_youbot_description)/robots/youbot_0.urdf.xacro" />
  <param name="robot_description_1" command="$(find xacro)/xacro.py $(find luh_youbot_description)/robots/youbot_1.urdf.xacro" /> -->


  <!-- Spawn a robot into Gazebo -->

  <!-- normal and multi-exit setting  -->
  <node name="spawn_urdf0" pkg="gazebo_ros" type="spawn_model"  args="-file $(find luh_youbot_description)/robots/youbot_0.urdf -urdf -x 0 -y -0.5 -z 0.1 -R 0 -P 0 -Y 1.57   -model youbot0" />
  <node name="spawn_urdf1" pkg="gazebo_ros" type="spawn_model"  args="-file $(find luh_youbot_description)/robots/youbot_1.urdf -urdf -x 0 -y -1.5 -z 0.1 -R 0 -P 0 -Y 1.57  -model youbot1" />

  <!-- <node name="spawn_urdf_0" pkg="gazebo_ros" type="spawn_model" args="-param robot_description_0 -urdf -x 0 -y -2 -z 0.1 -model youbot_0" /> -->
  <!-- <node name="spawn_urdf_1" pkg="gazebo_ros" type="spawn_model" args="-param robot_description_1 -urdf -x 2 -y -2 -z 0.1 -model youbot_1" /> -->
 
  <!-- special optimal case setting -->
  <!--
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -x 0 -y 2 -z 0.1 -model youbot" />
-->
  <!--node pkg="robot_state_publisher" type="robot_state_publisher" name="youbot_state_publisher" output="screen">
	<remap from="joint_states" to="gazebo/joint_states"/>
  </node-->

</launch>


