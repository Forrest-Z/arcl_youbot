<?xml version="1.0"?>
<package>
  <name>luh_youbot_gazebo</name>
  <version>0.0.0</version>
  <description>Contains Gazebo plugins for the youbot and a fake driver api for Gazebo</description>

  <maintainer email="info@luhbots.de">Simon Aden</maintainer>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>nav_msgs</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>setcap</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>std_srvs</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>roslib</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>arcl_youbot_kinematics</build_depend>
  <build_depend>luh_youbot_driver_api</build_depend>
  <build_depend>luh_youbot_gripper</build_depend>
  <build_depend>gazebo_ros</build_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>nav_msgs</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>std_srvs</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>roslib</run_depend>
  <run_depend>rospy</run_depend>
  <run_depend>arcl_youbot_kinematics</run_depend>
  <run_depend>luh_youbot_driver_api</run_depend>
  <run_depend>gazebo_ros</run_depend>
  <run_depend>luh_youbot_gripper</run_depend>

  <export>
    <gazebo_ros plugin_path="${prefix}/lib" gazebo_media_path="${prefix}" />
  </export>

</package>
